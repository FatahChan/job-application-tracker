{
  "{{chrome}}.manifest_version": 3,
  "{{firefox}}.manifest_version": 2,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "{{chrome}}.permissions": ["sidePanel"],
  "{{chrome}}.action": {
    "default_title": "Click to open panel"
  },
  "{{chrome}}.side_panel": {
    "default_path": "src/index.html"
  },
  "background": {
    "{{chrome}}.service_worker": "src/background.js"
  }
}
